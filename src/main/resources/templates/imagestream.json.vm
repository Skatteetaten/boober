{
  "kind": "ImageStream",
  "apiVersion": "v1",
  "metadata": {
    "name": "${adc.name}",
    "labels": {
      "app": "${adc.name}",
      "updatedBy": "${username}",
      "affiliation": "${adc.affiliation}",
      "releasedVersion": "${adc.version}",
      "booberDeployId": "${deployId}"

    }
  }
    #if (${adc.type} == "deploy")
    ,
      "spec": {
        "dockerImageRepository": "${dockerRegistry}/${adc.dockerGroup}/${adc.dockerName}",
        "tags": [
          {
            "name": "default",
              "from": {
                "kind": "DockerImage",
                #if ("$!{adc.releaseTo}" != "")
                  "name": "${dockerRegistry}/${adc.dockerGroup}/${adc.dockerName}:${adc.releaseTo}"
                #else
                  "name": "${dockerRegistry}/${adc.dockerGroup}/${adc.dockerName}:${adc.version}"
                #end
              },
              "importPolicy": {
                "scheduled": true
              }
          }
        ]
      }
    #end
}
