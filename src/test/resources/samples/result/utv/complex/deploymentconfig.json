{
  "apiVersion": "apps.openshift.io/v1",
  "kind": "DeploymentConfig",
  "metadata": {
    "annotations": {
      "boober.skatteetaten.no/applicationFile": "utv/complex.json",
      "console.skatteetaten.no/alarm": "true",
      "boober.skatteetaten.no/overrides": "{\"utv/complex.json\":{\"version\":\"1.0.4\"}}",
      "console.skatteetaten.no/management-path": ":8081/foobar"
    },
    "labels": {
      "app": "komplex",
      "updatedBy": "hero",
      "affiliation": "paas",
      "updateInBoober": "true",
      "name": "komplex",
      "deployTag": "1.0.4",
      "paused": "true"
    },
    "name": "komplex",
    "namespace": "paas-utv",
    "ownerReferences": [
      {
        "apiVersion": "skatteetaten.no/v1",
        "kind": "ApplicationDeployment",
        "name": "komplex",
        "uid": "123-123"
      }
    ]
  },
  "spec": {
    "replicas": 0,
    "selector": {
      "name": "komplex"
    },
    "strategy": {
      "recreateParams": {
        "timeoutSeconds": 180
      },
      "type": "Recreate"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "komplex",
          "updatedBy": "hero",
          "affiliation": "paas",
          "updateInBoober": "true",
          "name": "komplex",
          "deployTag": "1.0.4",
          "paused": "true"
        }
      },
      "spec": {
        "containers": [
          {
            "env": [
              {
                "name": "POD_NAME",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.name"
                  }
                }
              },
              {
                "name": "POD_NAMESPACE",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.namespace"
                  }
                }
              },
              {
                "name": "HTTP_PORT",
                "value": "8080"
              },
              {
                "name": "MANAGEMENT_HTTP_PORT",
                "value": "8081"
              },
              {
                "name": "JOLOKIA_HTTP_PORT",
                "value": "8778"
              },
              {
                "name": "OPENSHIFT_CLUSTER",
                "value": "utv"
              },
              {
                "name": "APP_NAME",
                "value": "komplex"
              },
              {
                "name": "SPLUNK_INDEX",
                "value": "test-index"
              },
              {
                "name": "ENABLE_REMOTE_DEBUG",
                "value": "true"
              },
              {
                "name": "DEBUG_PORT",
                "value": "5005"
              },
              {
                "name": "ROUTE_NAME",
                "value": "komplex-paas-utv.test.paas"
              },
              {
                "name": "ROUTE_URL",
                "value": "https://komplex-paas-utv.test.paas"
              },
              {
                "name": "COMPLEX_DB",
                "value": "/u01/secrets/app/complex-db/info"
              },
              {
                "name": "COMPLEX_DB_PROPERTIES",
                "value": "/u01/secrets/app/complex-db/db.properties"
              },
              {
                "name": "FOO_DB",
                "value": "/u01/secrets/app/foo-db/info"
              },
              {
                "name": "FOO_DB_PROPERTIES",
                "value": "/u01/secrets/app/foo-db/db.properties"
              },
              {
                "name": "DB",
                "value": "/u01/secrets/app/complex-db/info"
              },
              {
                "name": "DB_PROPERTIES",
                "value": "/u01/secrets/app/complex-db/db.properties"
              },
              {
                "name": "FOO",
                "valueFrom": {
                  "secretKeyRef": {
                    "key": "FOO",
                    "name": "komplex-foo-vault",
                    "optional": false
                  }
                }
              },
              {
                "name": "FOO",
                "value": "BAR"
              },
              {
                "name": "VOLUME_CONFIG",
                "value": "/u01/config/configmap"
              },
              {
                "name": "STS_CERTIFICATE_URL",
                "value": "/u01/secrets/app/komplex-cert/certificate.crt"
              },
              {
                "name": "STS_PRIVATE_KEY_URL",
                "value": "/u01/secrets/app/komplex-cert/privatekey.key"
              },
              {
                "name": "STS_KEYSTORE_DESCRIPTOR",
                "value": "/u01/secrets/app/komplex-cert/descriptor.properties"
              },
              {
                "name": "VOLUME_KOMPLEX_CERT",
                "value": "/u01/secrets/app/komplex-cert"
              },
              {
                "name": "VOLUME_AURORA_TOKEN_MOUNT",
                "value": "/u01/secrets/app/aurora-token"
              },
              {
                "name": "VOLUME_SECRET_MOUNT",
                "value": "/u01/foo"
              },
              {
                "name": "VOLUME_PVC_MOUNT",
                "value": "/foo"
              },
              {
                "name": "VOLUME_CONFIG_MOUNT",
                "value": "/content"
              }
            ],
            "imagePullPolicy": "IfNotPresent",
            "livenessProbe": {
              "initialDelaySeconds": 10,
              "tcpSocket": {
                "port": 8080
              },
              "timeoutSeconds": 1
            },
            "name": "komplex-java",
            "ports": [
              {
                "containerPort": 8080,
                "name": "http",
                "protocol": "TCP"
              },
              {
                "containerPort": 8081,
                "name": "management",
                "protocol": "TCP"
              },
              {
                "containerPort": 8778,
                "name": "jolokia",
                "protocol": "TCP"
              }
            ],
            "readinessProbe": {
              "initialDelaySeconds": 10,
              "tcpSocket": {
                "port": 8080
              },
              "timeoutSeconds": 1
            },
            "resources": {
              "limits": {
                "cpu": "2000m",
                "memory": "128Mi"
              },
              "requests": {
                "cpu": "100m",
                "memory": "64Mi"
              }
            },
            "securityContext": {
              "privileged": false
            },
            "terminationMessagePath": "/dev/termination-log",
            "volumeMounts": [
              {
                "mountPath": "/u01/logs",
                "name": "application-log-volume"
              },
              {
                "mountPath": "/u01/secrets/app/complex-db",
                "name": "komplex-complex-db"
              },
              {
                "mountPath": "/u01/secrets/app/foo-db",
                "name": "komplex-foo-db"
              },
              {
                "mountPath": "/u01/config/configmap",
                "name": "config"
              },
              {
                "mountPath": "/u01/secrets/app/komplex-cert",
                "name": "komplex-cert"
              },
              {
                "mountPath": "/u01/secrets/app/aurora-token",
                "name": "aurora-token-mount"
              },
              {
                "mountPath": "/u01/foo",
                "name": "secret-mount"
              },
              {
                "mountPath": "/foo",
                "name": "pvc-mount"
              },
              {
                "mountPath": "/content",
                "name": "config-mount"
              }
            ]
          }
        ],
        "dnsPolicy": "ClusterFirst",
        "restartPolicy": "Always",
        "serviceAccount": "aurora-fraggle",
        "volumes": [
          {
            "emptyDir": {},
            "name": "application-log-volume"
          },
          {
            "name": "komplex-complex-db",
            "secret": {
              "secretName": "komplex-complex-db"
            }
          },
          {
            "name": "komplex-foo-db",
            "secret": {
              "secretName": "komplex-foo-db"
            }
          },
          {
            "configMap": {
              "name": "komplex"
            },
            "name": "config"
          },
          {
            "name": "komplex-cert",
            "secret": {
              "secretName": "komplex-cert"
            }
          },
          {
            "name": "aurora-token-mount",
            "secret": {
              "secretName": "aurora-token-mount"
            }
          },
          {
            "name": "secret-mount",
            "secret": {
              "secretName": "komplex-secret-mount"
            }
          },
          {
            "name": "pvc-mount",
            "persistentVolumeClaim": {
              "claimName": "pvc-mount"
            }
          },
          {
            "configMap": {
              "name": "komplex-config-mount"
            },
            "name": "config-mount"
          }
        ]
      }
    },
    "triggers": [
      {
        "imageChangeParams": {
          "automatic": true,
          "containerNames": [
            "komplex-java"
          ],
          "from": {
            "kind": "ImageStreamTag",
            "name": "komplex:latest"
          }
        },
        "type": "ImageChange"
      }
    ]
  }
}