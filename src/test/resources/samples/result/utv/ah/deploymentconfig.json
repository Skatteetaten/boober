{
  "apiVersion": "v1",
  "kind": "DeploymentConfig",
  "metadata": {
    "annotations": {
      "boober.skatteetaten.no/applicationFile": "utv/ah.json",
      "console.skatteetaten.no/alarm": "true",
      "console.skatteetaten.no/management-path": ":8081/actuator"
    },
    "labels": {
      "name": "ah",
      "app": "ah",
      "updatedBy": "hero",
      "affiliation": "paas",
      "updateInBoober": "true",
      "deployTag": "1.3.4",
      "booberDeployId": "deploy1"
    },
    "name": "ah",
    "namespace": "paas-utv",
    "ownerReferences": [
      {
        "apiVersion": "skatteetaten.no/v1",
        "kind": "ApplicationDeployment",
        "name": "ah",
        "uid": "123-123"
      }
    ]
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "name": "ah"
    },
    "strategy": {
      "resources": {},
      "rollingParams": {
        "intervalSeconds": 1,
        "maxSurge": "25%",
        "maxUnavailable": 0,
        "timeoutSeconds": 120,
        "updatePeriodSeconds": 1
      },
      "type": "Rolling"
    },
    "template": {
      "metadata": {
        "labels": {
          "name": "ah",
          "app": "ah",
          "updatedBy": "hero",
          "affiliation": "paas",
          "updateInBoober": "true",
          "deployTag": "1.3.4"
        }
      },
      "spec": {
        "containers": [
          {
            "env": [
              {
                "name": "POD_NAME",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.name"
                  }
                }
              },
              {
                "name": "POD_NAMESPACE",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.namespace"
                  }
                }
              },
              {
                "name": "JAVA_OPTS",
                "value": "-Xmx4g -Xms2g"
              },
              {
                "name": "splunk_config_stanzas",
                "value": "# --- start/stanza STDOUT\n[monitor://./logs/*.log]\ndisabled = false\nfollowTail = 0\nsourcetype = log4j\nindex = INDEX-PLACEHOLDER\n_meta = environment::NAMESPACE-PLACEHOLDER application::ah nodetype::openshift\nhost = openshift-host\n# --- end/stanza\n\n# --- start/stanza ACCESS_LOG\n[monitor://./logs/*.access]\ndisabled = false\nfollowTail = 0\nsourcetype = access_combined\nindex = INDEX-PLACEHOLDER\n_meta = environment::NAMESPACE-PLACEHOLDER application::ah nodetype::openshift\nhost = openshift-host\n# --- end/stanza\n\n# --- start/stanza GC LOG\n[monitor://./logs/*.gc]\ndisabled = false\nfollowTail = 0\nsourcetype = gc_log\nindex = INDEX-PLACEHOLDER\n_meta = environment::NAMESPACE-PLACEHOLDER application::ah nodetype::openshift\nhost = openshift-host\n# --- end/stanza\n\n"
              },
              {
                "name": "FEED_NAME",
                "value": "feed"
              },
              {
                "name": "DB_NAME",
                "value": "feed"
              },
              {
                "name": "HOST_NAME",
                "value": "localhost"
              },
              {
                "name": "SCHEME",
                "value": "http"
              },
              {
                "name": "FOO",
                "value": "bar"
              },
              {
                "name": "AH_DB",
                "value": "/u01/secrets/app/ah-db/info"
              },
              {
                "name": "AH_DB_PROPERTIES",
                "value": "/u01/secrets/app/ah-db/db.properties"
              },
              {
                "name": "DB",
                "value": "/u01/secrets/app/ah-db/info"
              },
              {
                "name": "DB_PROPERTIES",
                "value": "/u01/secrets/app/ah-db/db.properties"
              },
              {
                "name": "OPENSHIFT_CLUSTER",
                "value": "utv"
              },
              {
                "name": "APP_NAME",
                "value": "ah"
              }
            ],
            "image": "ah",
            "imagePullPolicy": "IfNotPresent",
            "name": "ah",
            "ports": [
              {
                "containerPort": 8080,
                "protocol": "TCP"
              },
              {
                "containerPort": 8778,
                "name": "jolokia"
              }
            ],
            "resources": {
              "limits": {},
              "requests": {}
            },
            "securityContext": {
              "capabilities": {},
              "privileged": false
            },
            "terminationMessagePath": "/dev/termination-log",
            "volumeMounts": [
              {
                "mountPath": "/u01/logs",
                "name": "application-log-volume"
              },
              {
                "mountPath": "/u01/secrets/app/ah-db",
                "name": "ah-db"
              }
            ],
            "capabilities": {}
          }
        ],
        "dnsPolicy": "ClusterFirst",
        "restartPolicy": "Always",
        "serviceAccount": "",
        "serviceAccountName": "",
        "volumes": [
          {
            "emptyDir": {},
            "name": "application-log-volume"
          },
          {
            "name": "ah-db",
            "secret": {
              "secretName": "ah-db"
            }
          }
        ]
      }
    },
    "triggers": [
      {
        "imageChangeParams": {
          "automatic": true,
          "containerNames": [
            "ah"
          ],
          "from": {
            "kind": "ImageStreamTag",
            "name": "ah:default"
          },
          "lastTriggeredImage": ""
        },
        "type": "ImageChange"
      }
    ]
  }
}