{
  "apiVersion": "apps.openshift.io/v1",
  "kind": "DeploymentConfig",
  "metadata": {
    "annotations": {
      "boober.skatteetaten.no/applicationFile": "booberdev/reference.json",
      "console.skatteetaten.no/alarm": "true",
      "console.skatteetaten.no/management-path": ":8081/actuator"
    },
    "labels": {
      "app": "reference",
      "updatedBy": "hero",
      "affiliation": "aos",
      "updateInBoober": "true",
      "name": "reference",
      "deployTag": "1.0.0-b1.5.3-flange-8.152.18"
    },
    "name": "reference",
    "namespace": "aos-booberdev",
    "ownerReferences": [
      {
        "apiVersion": "skatteetaten.no/v1",
        "kind": "ApplicationDeployment",
        "name": "reference",
        "uid": "123-123"
      }
    ]
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "name": "reference"
    },
    "strategy": {
      "rollingParams": {
        "intervalSeconds": 1,
        "maxSurge": "25%",
        "maxUnavailable": 0,
        "timeoutSeconds": 180,
        "updatePeriodSeconds": 1
      },
      "type": "Rolling"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "reference",
          "updatedBy": "hero",
          "affiliation": "aos",
          "updateInBoober": "true",
          "name": "reference",
          "deployTag": "1.0.0-b1.5.3-flange-8.152.18"
        }
      },
      "spec": {
        "containers": [
          {
            "env": [
              {
                "name": "POD_NAME",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.name"
                  }
                }
              },
              {
                "name": "POD_NAMESPACE",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.namespace"
                  }
                }
              },
              {
                "name": "HTTP_PORT",
                "value": "8080"
              },
              {
                "name": "MANAGEMENT_HTTP_PORT",
                "value": "8081"
              },
              {
                "name": "JOLOKIA_HTTP_PORT",
                "value": "8778"
              },
              {
                "name": "OPENSHIFT_CLUSTER",
                "value": "utv"
              },
              {
                "name": "APP_NAME",
                "value": "reference"
              },
              {
                "name": "SPLUNK_INDEX",
                "value": "openshift-test"
              },
              {
                "name": "ROUTE_NAME",
                "value": "reference.ao.booberdev.utv.paas.skead.no"
              },
              {
                "name": "ROUTE_URL",
                "value": "http://reference.ao.booberdev.utv.paas.skead.no"
              },
              {
                "name": "STS_CERTIFICATE_URL",
                "value": "/u01/secrets/app/reference-cert/certificate.crt"
              },
              {
                "name": "STS_PRIVATE_KEY_URL",
                "value": "/u01/secrets/app/reference-cert/privatekey.key"
              },
              {
                "name": "STS_KEYSTORE_DESCRIPTOR",
                "value": "/u01/secrets/app/reference-cert/descriptor.properties"
              },
              {
                "name": "VOLUME_REFERENCE_CERT",
                "value": "/u01/secrets/app/reference-cert"
              }
            ],
            "imagePullPolicy": "IfNotPresent",
            "name": "reference-java",
            "ports": [
              {
                "containerPort": 8080,
                "name": "http",
                "protocol": "TCP"
              },
              {
                "containerPort": 8081,
                "name": "management",
                "protocol": "TCP"
              },
              {
                "containerPort": 8778,
                "name": "jolokia",
                "protocol": "TCP"
              }
            ],
            "readinessProbe": {
              "httpGet": {
                "path": "/foo",
                "port": 8080
              },
              "initialDelaySeconds": 10,
              "timeoutSeconds": 1
            },
            "resources": {
              "limits": {
                "cpu": "2000m",
                "memory": "512Mi"
              },
              "requests": {
                "cpu": "10m",
                "memory": "128Mi"
              }
            },
            "securityContext": {
              "privileged": false
            },
            "terminationMessagePath": "/dev/termination-log",
            "volumeMounts": [
              {
                "mountPath": "/u01/logs",
                "name": "application-log-volume"
              },
              {
                "mountPath": "/u01/secrets/app/reference-cert",
                "name": "reference-cert"
              }
            ]
          },
          {
            "args": [
              "-config",
              "/u01/config/config.json"
            ],
            "env": [
              {
                "name": "HTTP_PORT",
                "value": "8090"
              },
              {
                "name": "MANAGEMENT_HTTP_PORT",
                "value": "8474"
              }
            ],
            "image": "shopify/toxiproxy:2.1.3",
            "name": "reference-toxiproxy-sidecar",
            "ports": [
              {
                "containerPort": 8090,
                "name": "http",
                "protocol": "TCP"
              },
              {
                "containerPort": 8474,
                "name": "management",
                "protocol": "TCP"
              }
            ],
            "readinessProbe": {
              "initialDelaySeconds": 10,
              "tcpSocket": {
                "port": 8090
              },
              "timeoutSeconds": 1
            },
            "resources": {
              "limits": {
                "memory": "256Mi",
                "cpu": "1"
              },
              "requests": {
                "memory": "128Mi",
                "cpu": "10m"
              }
            },
            "volumeMounts": [
              {
                "mountPath": "/u01/config/configmap",
                "name": "reference-toxiproxy-config"
              }
            ]
          }
        ],
        "dnsPolicy": "ClusterFirst",
        "restartPolicy": "Always",
        "volumes": [
          {
            "emptyDir": {},
            "name": "application-log-volume"
          },
          {
            "configMap": {
              "name": "reference-toxiproxy-config"
            },
            "name": "reference-toxiproxy-config"
          },
          {
            "name": "reference-cert",
            "secret": {
              "secretName": "reference-cert"
            }
          }
        ]
      }
    },
    "triggers": [
      {
        "imageChangeParams": {
          "automatic": true,
          "containerNames": [
            "reference-java"
          ],
          "from": {
            "kind": "ImageStreamTag",
            "name": "reference:default"
          }
        },
        "type": "ImageChange"
      }
    ]
  }
}