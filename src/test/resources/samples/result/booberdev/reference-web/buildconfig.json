{
  "kind": "BuildConfig",
  "apiVersion": "v1",
  "metadata": {
    "name": "openshift-referanse-react",
    "labels": {
      "app": "openshift-referanse-react",
      "updatedBy": "hero",
      "affiliation": "aos",
      "booberDeployId": "123"
    }
  },
  "spec": {
    "triggers": [

    ],
    "strategy": {
      "type": "Custom",
      "customStrategy": {
        "from": {
          "kind": "ImageStreamTag",
          "namespace": "openshift",
          "name": "architect:1"
        },
        "env": [
          {
            "name": "ARTIFACT_ID",
            "value": "openshift-referanse-react"
          },
          {
            "name": "GROUP_ID",
            "value": "no.skatteetaten.aurora"
          },
          {
            "name": "VERSION",
            "value": "develop-SNAPSHOT"
          },
          {
            "name": "DOCKER_BASE_VERSION",
            "value": "0"
          },
          {
            "name": "DOCKER_BASE_IMAGE",
            "value": "aurora/wrench"
          },
          {
            "name": "PUSH_EXTRA_TAGS",
            "value": "latest,major,minor,patch"
          },
          {
            "name": "APPLICATION_TYPE",
            "value": "nodejs"
          }
        ],
        "exposeDockerSocket": true
      }
    },
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "openshift-referanse-react:latest"
      }
    }
  }
}
