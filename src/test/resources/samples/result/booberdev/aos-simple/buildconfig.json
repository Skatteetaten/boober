{
  "kind": "BuildConfig",
  "apiVersion": "v1",
  "metadata": {
    "name": "aos-simple",
    "labels": {
      "app": "aos-simple",
      "updatedBy": "hero",
      "affiliation": "aos",
      "booberDeployId": "123"
    }
  },
  "spec": {
    "triggers": [],
    "strategy": {
      "type": "Custom",
      "customStrategy": {
        "from": {
          "kind": "ImageStreamTag",
          "namespace": "openshift",
          "name": "architect:1"
        },
        "env": [
          {
            "name": "ARTIFACT_ID",
            "value": "aos-simple"
          },
          {
            "name": "GROUP_ID",
            "value": "ske.aurora.openshift"
          },
          {
            "name": "VERSION",
            "value": "1.0.3-SNAPSHOT"
          },
          {
            "name": "DOCKER_BASE_VERSION",
            "value": "8"
          },
          {
            "name": "DOCKER_BASE_IMAGE",
            "value": "aurora/wingnut"
          },
          {
            "name": "PUSH_EXTRA_TAGS",
            "value": "latest,major,minor,patch"
          }
        ],
        "exposeDockerSocket": true
      }
    },
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "aos-simple:latest"
      }
    }
  }
}