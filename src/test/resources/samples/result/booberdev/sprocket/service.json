{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "sprocket",
    "annotations": {
      "prometheus.io/scheme": "http",
      "prometheus.io/scrape": "true",
      "prometheus.io/path": "/prometheus",
      "prometheus.io/port": "8081"
    },
    "labels": {
      "app": "sprocket",
      "updatedBy": "hero",
      "affiliation": "aos"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "http",
        "protocol": "TCP",
        "port": 80,
        "targetPort": 8080,
        "nodePort": 0
      }
    ],
    "selector": {
      "name": "sprocket"
    },
    "type": "ClusterIP",
    "sessionAffinity": "None"
  }
}
