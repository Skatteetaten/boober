{
  "kind": "DeploymentConfig",
  "apiVersion": "v1",
  "metadata": {
    "annotations": {
      "boober.skatteetaten.no/applicationFile": "booberdev/console.json",
      "console.skatteetaten.no/alarm": "true",
      "console.skatteetaten.no/management-path": ":8082/actuator",
      "sprocket.sits.no/deployment-config.database": "openshift-console-api"
    },
    "labels": {
      "app": "openshift-console-api",
      "updatedBy": "hero",
      "affiliation": "aos",
      "booberDeployId": "123",
      "name": "openshift-console-api",
      "deployTag": "3"
    },
    "name": "openshift-console-api"
  },
  "spec": {
    "strategy": {
      "type": "Rolling",
      "rollingParams": {
        "intervalSeconds": 1,
        "maxSurge": "25%",
        "maxUnavailable": 0,
        "timeoutSeconds": 180,
        "updatePeriodSeconds": 1
      }
    },
    "triggers": [
      {
        "type": "ImageChange",
        "imageChangeParams": {
          "automatic": true,
          "containerNames": [
            "openshift-console-api"
          ],
          "from": {
            "name": "openshift-console-api:default",
            "kind": "ImageStreamTag"
          }
        }
      }
    ],
    "replicas": 1,
    "selector": {
      "name": "openshift-console-api"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "openshift-console-api",
          "updatedBy": "hero",
          "affiliation": "aos",
          "booberDeployId": "123",
          "name": "openshift-console-api",
          "deployTag": "3"
        }
      },
      "spec": {
        "volumes": [
          {
            "name": "openshift-console-api-db",
            "secret": {
              "secretName": "openshift-console-api-db"
            }
          },
          {
            "name": "application-log-volume",
            "emptyDir": {

            }
          }
        ],
        "containers": [
          {
            "name": "openshift-console-api",
            "ports": [
              {
                "containerPort": 8080,
                "protocol": "TCP",
                "name": "http"
              },
              {
                "containerPort": 8081,
                "protocol": "TCP",
                "name": "management"
              },
              {
                "containerPort": 8778,
                "name": "jolokia"
              }
            ],
            "env": [
              {
                "name": "POD_NAME",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.name"
                  }
                }
              },
              {
                "name": "POD_NAMESPACE",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.namespace"
                  }
                }
              },
              {
                "name": "OPENSHIFT_CLUSTER",
                "value": "utv"
              },
              {
                "name": "HTTP_PORT",
                "value": "8080"
              },
              {
                "name": "MANAGEMENT_HTTP_PORT",
                "value": "8081"
              },
              {
                "name": "APP_NAME",
                "value": "openshift-console-api"
              },
              {
                "name": "SPLUNK_INDEX",
                "value": "openshift-test"
              },
              {
                "name": "ROUTE_NAME",
                "value": "openshift-console-api-aos-booberdev.utv.paas.skead.no"
              },
              {
                "name": "ROUTE_URL",
                "value": "http://openshift-console-api-aos-booberdev.utv.paas.skead.no"
              },
              {
                "name": "REMOTE_DEBUG",
                "value": "true"
              },
              {
                "name": "DEBUG_PORT",
                "value": "5005"
              },
              {
                "name": "OPENSHIFT_CONSOLE_API_DB",
                "value": "/u01/secrets/app/openshift-console-api-db/info"
              },
              {
                "name": "OPENSHIFT_CONSOLE_API_DB_PROPERTIES",
                "value": "/u01/secrets/app/openshift-console-api-db/db.properties"
              },
              {
                "name": "DB",
                "value": "/u01/secrets/app/openshift-console-api-db/info"
              },
              {
                "name": "DB_PROPERTIES",
                "value": "/u01/secrets/app/openshift-console-api-db/db.properties"
              },
              {
                "name": "VOLUME_OPENSHIFT_CONSOLE_API_DB",
                "value": "/u01/secrets/app/openshift-console-api-db"
              }
            ],
            "resources": {
              "limits": {
                "cpu": "2000m",
                "memory": "128Mi"
              },
              "requests": {
                "cpu": "100m",
                "memory": "128Mi"
              }
            },
            "volumeMounts": [
              {
                "name": "openshift-console-api-db",
                "mountPath": "/u01/secrets/app/openshift-console-api-db"
              },
              {
                "name": "application-log-volume",
                "mountPath": "/u01/logs"
              }
            ],
            "terminationMessagePath": "/dev/termination-log",
            "imagePullPolicy": "IfNotPresent",
            "capabilities": {
            },
            "securityContext": {
              "capabilities": {
              },
              "privileged": false
            },
            "readinessProbe": {
              "tcpSocket": {
                "port": 8080
              },
              "initialDelaySeconds": 10,
              "timeoutSeconds": 1
            }
          }
        ],
        "restartPolicy": "Always",
        "dnsPolicy": "ClusterFirst"
      }
    }
  }
}