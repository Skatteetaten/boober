{
  "version": "1.0.3-SNAPSHOT",
  "config": {
    "latest": {
      "foo": "baaaar"
    }
  },
  "segment": "aurora",
  "route": {
    "@name@": {
      "host": "@segment@.@env@"
    },
    "bar": {
      "path": "/bar",
      "host": "@segment@.@env@",
      "annotations": {
        "haproxy.router.openshift.io|timeout": "30s",
        "wembley.sits.no|apiPathsWithScopes": [
          {
            "path": "/foo/bar",
            "scopes": "TEST, TEST1"
          },
          {
            "path": "/foo/baz",
            "scopes": "FOO, BAR"
          }
        ]
      }
    }
  }
}
