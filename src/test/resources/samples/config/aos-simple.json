{
  "certificate": true,
  "deployStrategy": {
    "type": "rolling"
  },
  "groupId": "ske.aurora.openshift",
  "artifactId": "aos-simple",
  "name": "aos-simple",
  "version": "1.0.3",
  "splunkIndex": "openshift-test",
  "resources": {
    "memory": {
      "max": "128Mi"
    }
  },
  "webseal": {
    "host": "test",
    "roles": "admin dev"
  },
  "route": {    
    "aos-simple": {
      "path": "/foo",
      "annotations": {
        "haproxy.router.openshift.io|timeout": "30s"
      }
    }
  },
  "management": false,
  "prometheus": {
    "path": ""
  }
}
