{
  "name": "komplex",
  "version": "1",
  "groupId": "org.test",
  "type": "deploy",
  "splunkIndex": "test-index",
  "logging": {
    "index": "test-index"
  },
  "prometheus": {
    "path": "/prometheus",
    "port": "8082"
  },
  "management": {
    "port": "8081",
    "path": "/foobar"
  },
  "nodeSelector": [
    "node-role.kubernetes.io/largemem=true"
  ],
  "serviceAccount": "aurora-fraggle",
  "mounts": {
    "aurora-token": {
      "path": "/u01/secrets/app/aurora-token",
      "type": "Secret",
      "exist": true
    },
    "secret-mount": {
      "type": "Secret",
      "path": "/u01/foo",
      "secretVault": "foo"
    },
    "pvc": {
      "type": "PVC",
      "exist": true,
      "path": "/foo"
    }
  },
  "resources": {
    "memory": {
      "max": "128Mi",
      "min": "64Mi"
    },
    "cpu": {
      "max": "2000m",
      "min": "100m"
    }
  },
  "webseal": {
    "host": "test",
    "roles": "admin,dev,@name@"
  },
  "config": {
    "FOO": "BAR",
    "THIS.VALUE": "BAZ"
  },
  "debug": true,
  "route": {
    "@name@": {
      "tls": {
        "termination": "edge",
        "insecurePolicy": "Allow"
      }
    },
    "path": {
      "host": "pathroute",
      "path": "/path"
    },
    "foobar": {
      "host": "foobar",
      "annotations": {
        "wembley.sits.no|externalHost": "test.ske",
        "wembley.sits.no|apiPaths": "/api",
        "haproxy.router.openshift.io|timeout": "30s"
      }
    }
  },
  "bigip": {
    "service": "simple-utv"
  },
  "alerts": {
    "alert": {
      "enabled": true,
      "expr": "test-expression",
      "delay": "1",
      "connection": "mattermost",
      "severity": "warning"
    }
  },
  "topology": {
    "partOf": "test-group",
    "runtime": "spring-boot",
    "connectsTo": [
      "test-app1",
      "test-app2"
    ]
  },
  "liveness": true,
  "releaseTo": "komplex",
  "deployStrategy": {
    "type": "recreate"
  },
  "ttl": "1d",
  "pause": true,
  "message": "Aurora <3",
  "s3": {
    "default": {
      "bucketName": "mybucket"
    }
  },
  "database": {
    "complex": {
      "enabled": true,
      "tryReuse": true,
      "ignoreMissingSchema": false
    },
    "foo": "123-456",
    "missingdb": {
      "enabled": true,
      "generate": false
    }
  },
  "certificate": {
    "commonName": "komplex"
  },
  "sts": true,
  "secretVaults": {
    "foo": {
      "keys": [
        "BAR",
        "FOO"
      ],
      "keyMappings": {
        "BAR": "BAZ"
      }
    }
  },
  "notification": {
    "email": {
      "foo@bar.no": {
        "enabled": true
      },
      "ola@nordmann.net": {
        "enabled": true
      }
    },
    "mattermost": {
      "test": {
        "enabled": true
      },
      "test2": {
        "enabled": true
      }
    }
  },
  "azure": {
    "azureAppFqdn": "complex-@env@.amutv.skead.no",
    "groups": [],
    "jwtToStsConverter": {
      "enabled": true
    }
  }
}
