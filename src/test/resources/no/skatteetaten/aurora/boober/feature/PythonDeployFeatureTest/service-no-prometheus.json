{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "annotations": {
      "prometheus.io/scrape": "false"
    },
    "name": "simple",
    "namespace": "paas-utv"
  },
  "spec": {
    "ports": [
      {
        "name": "http",
        "port": 80,
        "protocol": "TCP",
        "targetPort": 8080
      },      {
        "name": "extra",
        "port": 8082,
        "protocol": "TCP",
        "targetPort": 8082
      }
    ],
    "selector": {
      "name": "simple"
    },
    "sessionAffinity": "None",
    "type": "ClusterIP"
  }
}