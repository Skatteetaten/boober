{
  "kind": "DeploymentConfig",
  "apiVersion": "v1",
  "metadata": {
    "annotations": {
      "boober.skatteetaten.no/applicationFile": "imagestreamtest/reference.json",
      "console.skatteetaten.no/alarm": "true",
      "console.skatteetaten.no/management-path": ":8081/actuator",
      "sprocket.sits.no/deployment-config.certificate": "no.skatteetaten.aurora.openshift.reference"
    },
    "labels": {
      "app": "reference",
      "updatedBy": "hero",
      "affiliation": "aos",
      "booberDeployId": "b6dfcef1-b80f-4087-be87-547266b0fcdf",
      "name": "reference",
      "deployTag": "1"
    },
    "name": "reference",
    "namespace": "aos-booberdev"
  },
  "spec": {
    "strategy": {
      "type": "Rolling",
      "rollingParams": {
        "intervalSeconds": 1,
        "maxSurge": "25%",
        "maxUnavailable": 0,
        "timeoutSeconds": 180,
        "updatePeriodSeconds": 1
      }
    },
    "triggers": [
      {
        "type": "ImageChange",
        "imageChangeParams": {
          "automatic": true,
          "containerNames": [
            "reference"
          ],
          "from": {
            "name": "reference:default",
            "kind": "ImageStreamTag"
          },
          "lastTriggeredImage": "docker-registry.aurora.sits.no:5000/no_skatteetaten_aurora_openshift/openshift-reference-springboot-server@sha256:c43f2d51969457a6a085c0fb921ac22090588ca254a78014bf62834baf"
        }
      }
    ],
    "replicas": 1,
    "selector": {
      "name": "reference"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "reference",
          "updatedBy": "hero",
          "affiliation": "aos",
          "booberDeployId": "b6dfcef1-b80f-4087-be87-547266b0fcdf",
          "name": "reference",
          "deployTag": "1"
        }
      },
      "spec": {
        "volumes": [
          {
            "name": "reference-cert",
            "secret": {
              "secretName": "reference-cert"
            }
          },
          {
            "name": "application-log-volume",
            "emptyDir": {}
          }
        ],
        "containers": [
          {
            "name": "reference",
            "ports": [
              {
                "containerPort": 8080,
                "protocol": "TCP",
                "name": "http"
              },
              {
                "containerPort": 8081,
                "protocol": "TCP",
                "name": "management"
              },
              {
                "containerPort": 8778,
                "name": "jolokia"
              }
            ],
            "env": [
              {
                "name": "POD_NAME",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.name"
                  }
                }
              },
              {
                "name": "POD_NAMESPACE",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.namespace"
                  }
                }
              },
              {
                "name": "OPENSHIFT_CLUSTER",
                "value": "utv"
              },
              {
                "name": "HTTP_PORT",
                "value": "8080"
              },
              {
                "name": "MANAGEMENT_HTTP_PORT",
                "value": "8081"
              },
              {
                "name": "APP_NAME",
                "value": "reference"
              },
              {
                "name": "SPLUNK_INDEX",
                "value": "openshift-test"
              },
              {
                "name": "ROUTE_NAME",
                "value": "reference-aos-imagestreamtest.utv.paas.skead.no"
              },
              {
                "name": "ROUTE_URL",
                "value": "http://reference-aos-imagestreamtest.utv.paas.skead.no"
              },
              {
                "name": "STS_CERTIFICATE_URL",
                "value": "/u01/secrets/app/reference-cert/certificate.crt"
              },
              {
                "name": "STS_PRIVATE_KEY_URL",
                "value": "/u01/secrets/app/reference-cert/privatekey.key"
              },
              {
                "name": "STS_KEYSTORE_DESCRIPTOR",
                "value": "/u01/secrets/app/reference-cert/descriptor.properties"
              },
              {
                "name": "VOLUME_REFERENCE_CERT",
                "value": "/u01/secrets/app/reference-cert"
              }
            ],
            "resources": {
              "limits": {
                "cpu": "2000m",
                "memory": "512Mi"
              },
              "requests": {
                "cpu": "100m",
                "memory": "128Mi"
              }
            },
            "volumeMounts": [
              {
                "name": "reference-cert",
                "mountPath": "/u01/secrets/app/reference-cert"
              },
              {
                "name": "application-log-volume",
                "mountPath": "/u01/logs"
              }
            ],
            "terminationMessagePath": "/dev/termination-log",
            "imagePullPolicy": "IfNotPresent",
            "capabilities": {},
            "securityContext": {
              "capabilities": {},
              "privileged": false
            },
            "readinessProbe": {
              "tcpSocket": {
                "port": 8080
              },
              "initialDelaySeconds": 10,
              "timeoutSeconds": 1
            }
          }
        ],
        "restartPolicy": "Always",
        "dnsPolicy": "ClusterFirst"
      }
    }
  }
}