{
  "affiliation": "paas",
  "setupParams": {
    "apps": [
      "refapp"
    ],
    "envs": [
      "wingnut"
    ],
    "overrides": {}
  },
  "auroraConfig": {
    "files": {
      "about.json": {
        "schemaVersion": "v1",
        "affiliation": "paas",
        "type": "deploy",
        "permissions" : {
          "admin" : {
            "groups": "APP_PaaS_drift APP_PaaS_utv"
          }
        }
      },
      "refapp.json": {
        "name": "refapp",
        "replicas": "1",
        "flags": {
          "rolling": true,
          "cert": true,
          "route": true
        },
        "groupId": "ske.aurora.openshift",
        "artifactId": "openshift-reference-springboot-server",
        "version": "1",
        "managementPath": ":8081/actuator",
        "database": "referanseapp",
        "prometheus" : {
          "port": "8081",
          "path": "/prometheus"
        }
      },
      "wingnut/about.json": {
        "cluster": "utv"
      },
      "wingnut/refapp.json": {
      }
    }
  }
}
